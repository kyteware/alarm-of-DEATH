{
    "manifest_version": 3,
    "name": "Search History Fetcher",
    "version": "1.0",
    "description": "Fetches your browser history to help you wake up",
    "permissions": ["history"],
    "host_permissions": ["http://localhost:5000/*"],
    "background": {
      "service_worker": "background.js"
    }
  }